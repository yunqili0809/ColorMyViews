<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" style="@android:style/Theme.Black">
    <TextView
            android:layout_width="326dp"
            android:layout_height="489dp"
            android:id="@+id/box_two_text"
            app:layout_constraintStart_toStartOf="parent"
            style="@style/RedBox" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0" android:layout_marginTop="8dp" android:layout_marginEnd="8dp"/>
    <TextView
            android:layout_width="6dp"
            android:layout_height="712dp"
            android:id="@+id/textView3" app:layout_constraintEnd_toStartOf="@+id/box_two_text"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="1.0"
            style="@style/Black"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:layout_width="61dp"
            android:layout_height="16dp" tools:layout_editor_absoluteY="222dp"
            android:id="@+id/textView5" app:layout_constraintEnd_toStartOf="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" style="@style/Black"/>
    <TextView
            android:text="TextView"
            android:layout_width="394dp"
            android:layout_height="8dp"
            android:id="@+id/textView10" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent" style="@style/Black"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintTop_toBottomOf="@+id/box_two_text"/>
    <TextView
            android:layout_width="61dp"
            android:layout_height="219dp"
            android:id="@+id/textView11"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" style="@style/BlueBox"
            app:layout_constraintEnd_toStartOf="@+id/textView3"/>
    <TextView
            android:layout_width="8dp"
            android:layout_height="227dp"
            tools:layout_editor_absoluteX="324dp" android:id="@+id/textView12" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/box_two_text" style="@style/Black"/>
    <TextView
            android:layout_width="71dp"
            android:layout_height="16dp"
            android:id="@+id/textView13" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/box_two_text"
            app:layout_constraintBottom_toBottomOf="@+id/textView12"
            style="@style/Black" app:layout_constraintStart_toEndOf="@+id/textView12"
    />
    <TextView
            android:layout_width="70dp"
            android:layout_height="107dp"
            tools:layout_editor_absoluteX="332dp" android:id="@+id/textView14" style="@style/OrangeBox"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView13"/>
</androidx.constraintlayout.widget.ConstraintLayout>